{
    "manifest_version": 3,
    "name": "Wellness Extension",
    "description": "Wellness and Productivity Extension",
    "version": "1.0",
    "background": {
        "type": "module",
        "service_worker": "build/background.bundle.js"
    },
    "action": {
        "default_popup": "build/index.html",
        "default_icon": "src/public/logo192.png"
    },
    "permissions": [
        "activeTab", "tabs"
    ]
}
